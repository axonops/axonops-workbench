{
  "name": "axonops-workbench-cassandra",
  "title": "AxonOps™ Workbench Cassandra®",
  "version": "0.1.0-1",
  "author": {
    "name": "AxonOps",
    "email": "info@axonops.com"
  },
  "main": "main/main.js",
  "license": "Apache-2.0",
  "description": "AxonOps™ Workbench Cassandra®",
  "productName": "AxonOpsCassandra",
  "maintainers": [
    "info@axonops.com"
  ],
  "build": {
    "icon": "./renderer/assets/images/icon-colored.png",
    "copyright": "AxonOps Limited 2024",
    "appId": "com.axonops.workbench.cassandra",
    "compression": "maximum",
    "extraResources": [
      "main/bin",
      "data",
      "config",
      "localization"
    ],
    "publish": [{
      "provider": "github"
    }],
    "win": {
      "icon": "./renderer/assets/images/icon-colored.ico",
      "target": [
        "nsis",
        "zip"
      ],
      "verifyUpdateCodeSignature": false,
      "win": {
        "sign": false
      }
    },
    "linux": {
      "icon": "./renderer/assets/images/icon-colored.png",
      "category": "Development",
      "desktop": "AxonOps Workbench Cassandra",
      "description": "AxonOps™ Workbench Cassandra®"
    },
    "mac": {
      "icon": "./renderer/assets/images/icon-colored.icns",
      "category": "public.app-category.developer-tools",
      "extendInfo": {
        "ElectronTeamID": "UJ776LUP23"
      },
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "type": "distribution",
      "entitlements": "./resources/entitlements.mac.plist",
      "entitlementsInherit": "./resources/entitlements.mac.plist",
      "provisioningProfile": "./resources/workbenchcassandra.provisionprofile",
      "extraFiles": [
        "./resources/workbenchcassandra.provisionprofile"
      ],
      "notarize": {
        "teamId": "UJ776LUP23"
      },
      "binaries": [
        "main/bin/cqlsh-407",
        "main/bin/cqlsh-410",
        "main/bin/keys_generator"
      ]
    },
    "deb": {
      "depends": [
        "libnss3",
        "libsecret-1-0"
      ]
    },
    "dmg": {
      "sign": false
    }
  },
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -w -f",
    "dev": "electron --trace-warnings --inspect .",
    "win": "electron-builder --win --publish onTag --x64",
    "win:arm64": "electron-builder --win --publish onTag --arm64",
    "tar": "electron-builder --linux tar.gz --publish onTag",
    "deb": "electron-builder --linux deb --publish onTag",
    "rpm": "electron-builder --linux rpm --publish onTag",
    "snap": "electron-builder --linux snap --publish onTag",
    "linux": "electron-builder --linux tar.gz deb rpm --publish onTag",
    "linux:arm64": "electron-builder --linux tar.gz deb rpm --publish onTag --arm64",
    "mac": "electron-builder --mac dmg pkg zip --publish onTag --x64",
    "mac:arm64": "electron-builder --mac dmg pkg zip --publish onTag --arm64",
    "mac:zip": "electron-builder --mac zip --publish onTag",
    "mac:pkg": "electron-builder --mac pkg --publish onTag"
  },
  "repository": "https://github.com/axonops/axonops-developer-workbench-cassandra",
  "devDependencies": {
    "electron": "^30.0.9",
    "electron-builder": "24.12.0",
    "electron-rebuild": "^3.2.9",
    "license-checker": "^25.0.1"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^3.1.0"
  },
  "dependencies": {
    "@ramumb/strip-tags": "^0.1.3",
    "ansi-to-html": "^0.7.2",
    "appdata-path": "^1.0.0",
    "autosize": "^6.0.1",
    "byte-size": "^8.1.1",
    "better-sqlite3": "^11.1.2",
    "chart.js": "^4.3.0",
    "co": "^4.6.0",
    "configparser": "^0.3.10",
    "debug": "^4.3.5",
    "diff": "^5.1.0",
    "electron-context-menu": "^3.6.1",
    "electron-positioner": "^4.1.0",
    "escape-html": "^1.0.3",
    "extract-string": "^1.0.2",
    "fast-string-search": "^1.4.3",
    "fs-extra": "^11.1.1",
    "hex-to-rgb": "^1.0.1",
    "i18next": "^22.4.9",
    "id-16": "^1.0.5",
    "jquery": "^3.6.3",
    "jquery.json-viewer": "^1.5.0",
    "js-yaml": "^4.1.0",
    "json-beautify": "^1.1.1",
    "json-to-table": "^4.2.1",
    "jsonfile": "^6.1.0",
    "jsonrepair": "^3.0.2",
    "keytar": "^7.9.0",
    "markdown": "^0.5.0",
    "material-components-web": "^14.0.0",
    "material-icons": "^1.13.1",
    "monaco-editor": "^0.34.1",
    "node-cmd": "^5.0.0",
    "node-machine-id": "^1.1.12",
    "node-pty": "^1.0.0",
    "node-rsa": "^1.1.1",
    "open": "^8.4.0",
    "port-get": "^1.0.0",
    "random-flat-colors": "^1.0.7",
    "regedit-simple": "^0.0.4",
    "sanitize-filename": "^1.6.3",
    "sort-json": "^2.0.1",
    "ssh2": "^1.15.0",
    "store": "^2.0.12",
    "stripchar": "^1.2.1",
    "systeminformation": "^5.18.2",
    "tabulator-tables": "^6.2.1",
    "tinycolor2": "^1.6.0",
    "tree-kill": "^1.2.2",
    "v8-compile-cache": "^2.3.0",
    "value-size": "^1.0.1",
    "xterm": "^5.2.1",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-theme": "^1.1.0",
    "xterm-webfont": "^2.0.0",
    "yargs": "^17.7.2",
    "node-gyp": "^10.1.0"
  }
}
