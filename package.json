    "port-get": "^1.0.0",
    "random-flat-colors": "^1.0.7",
    "regedit-simple": "^0.0.4",
    "sanitize-filename": "^1.6.3",
    "sort-json": "^2.0.1",
    "ssh2": "^1.14.0",
    "stripchar": "^1.2.1",
    "systeminformation": "^5.18.2",
    "tinycolor2": "^1.6.0",
    "tree-kill": "^1.2.2",
    "typed.js": "^2.0.16",
    "v8-compile-cache": "^2.3.0",
    "value-size": "^1.0.1",
    "xterm": "^5.2.1",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-web-links": "^0.8.0",
